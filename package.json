    "request": "^2.88.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.3",
    "nyc": "^14.1.1"
  }

{
  "name": "@ovalcms/backend-node",
  "version": "0.0.2",
  "description": "Example backend node server to read from the OvalCMS API and respond to a given frontend client.",
  "main": "app.js",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/ovalcms/backend-node.git"
  },
  "keywords": [
    "api",
    "backend",
    "cms",
    "example",
    "node",
    "oval",
    "ovalcms",
    "server"
  ],
  "scripts": {
